<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PersonalSoundboard.GridPage">
	<Grid Padding="10">
		<Grid.RowDefinitions>
			<RowDefinition Height="*" />
			<RowDefinition Height="*" />
		</Grid.RowDefinitions>
		<Grid.ColumnDefinitions>
			<ColumnDefinition Width="*" />
			<ColumnDefinition Width="*" />
		</Grid.ColumnDefinitions>
		<Label BackgroundColor="Red" Text="Test" Grid.Row="0" Grid.Column="0"/>
		<Label BackgroundColor="Red" Text="Test" Grid.Row="0" Grid.Column="1"/>
		<Label BackgroundColor="Red" Text="Test" Grid.Row="0" Grid.Column="2"/>
		<Label BackgroundColor="Red" Text="Test" Grid.Row="0" Grid.Column="3"/>
		<Label BackgroundColor="Red" Text="Test" Grid.Row="1" Grid.Column="0"/>
		<Label BackgroundColor="Red" Text="Test" Grid.Row="1" Grid.Column="1"/>
		<Label BackgroundColor="Red" Text="Test" Grid.Row="1" Grid.Column="2"/>
		<Label BackgroundColor="Red" Text="Test" Grid.Row="1" Grid.Column="3"/>
		<Label BackgroundColor="Red" Text="Test" Grid.Row="2" Grid.Column="0"/>
		<Label BackgroundColor="Red" Text="Test" Grid.Row="2" Grid.Column="1"/>
		<Label BackgroundColor="Red" Text="Test" Grid.Row="2" Grid.Column="2"/>
		<Label BackgroundColor="Red" Text="Test" Grid.Row="2" Grid.Column="3"/>
	</Grid>
</ContentPage>