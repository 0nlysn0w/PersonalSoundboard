<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PersonalSoundboard.GridPage">
    <Grid Padding="5">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Button  Text="Test" Grid.Row="0" Grid.Column="0"/>
		<Button  Text="Test" Grid.Row="0" Grid.Column="1"/>
        <Button  Text="Test" Grid.Row="0" Grid.Column="2"/>
        <Button  Text="Test" Grid.Row="1" Grid.Column="0"/>
		<Button  Text="Test" Grid.Row="1" Grid.Column="1"/>
        <Button  Text="Test" Grid.Row="1" Grid.Column="2"/>
        <Button  Text="Test" Grid.Row="2" Grid.Column="0"/>
		<Button  Text="Test" Grid.Row="2" Grid.Column="1"/>
		<Button  Text="Test" Grid.Row="2" Grid.Column="2"/>
	</Grid>
</ContentPage>